<core:View 
	xmlns:core="sap.ui.core" 
	xmlns="sap.m" 
	xmlns:commons="sap.ui.commons" 
	xmlns:l="sap.ui.layout" 
	xmlns:f="sap.ui.layout.form"
	controllerName="sap.ui.demo.logbook.view.detailPage" 
	viewName="sap.ui.demo.logbook.view.detailPage"
	id="detailView"
	>
    <Page xmlns="sap.m"
		class="sapUiFioriObjectPage"
    	id="detailPage"
    	title="Trips in 2014"
		showNavButton="{device>/isPhone}"
    	showHeader="true"
    	navButtonText=""
    	enableScrolling="true"
    	icon=""
    	navButtonType="Back"
    	showFooter="true"
    	navButtonTap="onDetailBack"
    	navButtonPress="onDetailBack"
    	>
			<tooltip></tooltip> <!-- sap.ui.core.TooltipBase -->
			<dependents></dependents> <!-- sap.ui.core.Control, since 1.19 -->
			<content>
    			<l:Grid
    		        defaultSpan="L12 M12 S12"
    		        hSpacing="2"
    		        width="auto">
                    <l:content>
        		        <f:Form id="Form1"
        		            minWidth="1024"
        		            maxContainerCols="2"
        		            editable="false"
        		            class="isReadOnly">
        		            <f:title>
        		                <core:Title text="So many trips in {year}" />
        		            </f:title>
                            <f:layout>
                              <f:ResponsiveGridLayout
                                labelSpanL="3"
                                labelSpanM="3"
                                emptySpanL="4"
                                emptySpanM="4"
                                columnsL="1"
                                columnsM="1" />
                            </f:layout>
                            <f:formContainers>
                              <f:FormContainer>
                                <f:formElements>
                                  <f:FormElement label="Name">
                                    <f:fields>
                                      <Text text="{SupplierName}" />
                                    </f:fields>
                                  </f:FormElement>
                                </f:formElements>
                              </f:FormContainer>
                            </f:formContainers>
        	            </f:Form>
    	            </l:content>
    		    </l:Grid>
			</content> <!-- sap.ui.core.Control -->
			<customHeader></customHeader> <!-- sap.m.IBar -->
			<footer></footer> <!-- sap.m.IBar -->
			<subHeader></subHeader> <!-- sap.m.IBar -->
			<headerContent></headerContent> <!-- sap.ui.core.Control -->
	</Page>
	
</core:View>