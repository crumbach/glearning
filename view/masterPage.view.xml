<core:View 
	xmlns:core="sap.ui.core" 
	xmlns="sap.m" 
	xmlns:commons="sap.ui.commons" 
	xmlns:layout="sap.ui.layout" 
	controllerName="sap.ui.demo.logbook.view.masterPage" 
	viewName="sap.ui.demo.logbook.view.masterPage"
	id="masterView"
	>
    <Page xmlns="sap.m"
    	id="masterPage"
    	title="Trips per Year"
    	showNavButton="true"
    	showHeader="true"
    	navButtonText=""
    	enableScrolling="true"
    	icon=""
    	backgroundDesign="Standard"
    	navButtonType="Back"
    	showFooter="true"
    	navButtonPress="quitSplitApp">
    		<tooltip></tooltip> <!-- sap.ui.core.TooltipBase -->
    		<dependents></dependents> <!-- sap.ui.core.Control, since 1.19 -->
    		<content>			    
    		    <List xmlns="sap.m"
    		    	id="logbookList"
    		    	mode="SingleSelectMaster"
                    items="{/logbooks}"
    		    	noDataText="no data available"
    		    	showNoData="true" 
    		    	select="onSelect" 
    		    	>
		    		<tooltip></tooltip> <!-- sap.ui.core.TooltipBase -->
		    		<dependents></dependents> <!-- sap.ui.core.Control, since 1.19 -->
		    		<swipeContent></swipeContent> <!-- sap.ui.core.Control -->
		    		<headerToolbar></headerToolbar> <!-- sap.m.Toolbar, since 1.16 -->
		    		<infoToolbar></infoToolbar> <!-- sap.m.Toolbar, since 1.16 -->
		    		<columns></columns> <!-- sap.m.Column -->
    				<items>
    					<ObjectListItem
    						type="Active"
    						press="onSelect"
    						title="Trips in {year}"
    						number="{number}"
    						numberUnit="trips">
    					</ObjectListItem>
    				</items>
    		    </List>
            </content> <!-- sap.ui.core.Control -->
    		<customHeader></customHeader> <!-- sap.m.Bar -->
    		<footer>
    			<Toolbar>
                    <ToolbarSpacer/>
                    <Button icon="sap-icon://add" tooltip="Log new trip" type="Emphasized" press="onNewTrip"/>
                </Toolbar>
    		</footer> <!-- sap.m.Bar -->
    		<subHeader></subHeader> <!-- sap.m.Bar -->
    		<headerContent></headerContent> <!-- sap.ui.core.Control -->
    </Page>
</core:View>

